Section:NewToggle("Spam Trap All Players", "", function(state)
    if state then
        LoopTrap = RS.RenderStepped:Connect(function()
        
for _, v in pairs(game.Players:GetChildren()) do
if v ~= game.Players.LocalPlayer then
if v.Character then
if v.Character:FindFirstChild("HumanoidRootPart") then
game.ReplicatedStorage.TrapSystem.PlaceTrap:InvokeServer(v.Character.HumanoidRootPart.CFrame * CFrame.new(0, -1, 0))
end
end
end
end
end)
    else
        LoopTrap:Disconnect()
    end
end)



